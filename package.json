    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/harshadakardil/Python-tutor/issues"
    },
    "homepage": "https://github.com/harshadakardil/Python-tutor#readme",
    "dependencies": {
        "@jridgewell/sourcemap-codec": "^1.5.0",
        "concurrently": "^8.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "node-fetch": "^2.6.9",
        "rimraf": "^6.0.1"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "esbuild": "^0.25.0",
        "nodemon": "^3.1.9",
