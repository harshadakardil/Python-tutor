{
    "name": "python-tutor",
    "version": "1.0.0",
    "description": "This is a Python project for learning and tutoring.",
    "main": "server.js",
    "scripts": {
        "build": "cd frontend && npm install && npm run build",
        "start": "node server.js",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && python app.py",
        "dev:proxy": "nodemon server.js",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:proxy\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/harshadakardil/Python-tutor.git"
    },
    "keywords": [],
    "author": "",
